# rename application.properties(Sample) To application.properties
spring.jpa.hibernate.ddl-auto=update
spring.jpa.generate-ddl=true
spring.jpa.database-platform=DATABASE_DIALECT
spring.application.name=Money Meter
spring.datasource.driverClassName=DATABASE_DRIVER
spring.datasource.url=STRING_URI_DATABASE
spring.datasource.username=DATABASE_USERNAME
spring.datasource.password=DATABASE_PASSWORD

